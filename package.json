{
  "name": "emr-exporter",
  "version": "1.0.0",
  "description": "Vendor agnostic application to export Universal Schema data from an Electronic Medical Record database and securely transfer results to a remote server.",
  "main": "index.js",
  "scripts": {
    "depcheck": "depcheck",
    "stylecheck": "eslint **/*.js",
    "start": "node index.js",
    "build": "pkg . --out-path ./build/",
    "clean": "rm -rf ./build",
    "check": "check-audit && npm run depcheck && npm outdated && npm run stylecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HDCbc/emr-exporter.git"
  },
  "keywords": [
    "HDC",
    "Exporter"
  ],
  "author": "Jonathan Zacharuk <jzacharuk@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/HDCbc/emr-exporter/issues"
  },
  "homepage": "https://github.com/HDCbc/emr-exporter#readme",
  "dependencies": {
    "archiver": "^2.1.0",
    "async": "^2.6.0",
    "dotenv": "^4.0.0",
    "ip": "^1.1.5",
    "joi": "^13.0.2",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.25",
    "mysql": "^2.15.0",
    "nconf": "^0.10.0",
    "node-forge": "^0.7.1",
    "pg": "^7.4.0",
    "printf": "^0.2.5",
    "progress-stream": "^2.0.0",
    "readline-sync": "^1.4.9",
    "rimraf": "^2.6.2",
    "ssh2": "^0.5.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "depcheck": "^0.6.8",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0"
  },
  "pkg": {
    "assets": [
      "mappings/*.json",
      "examples/*.env"
    ]
  },
  "bin": "./index.js"
}
